{
  "name": "stocksalesappdevelopemttype",
  "version": "115.0.0",
  "description": "Stock and Sales Management System with Electron + Node.js/Express backend",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "server": "node server.js",
    "build:tailwind": "tailwindcss -i ./src/input.css -o ./styles/output.css",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:appx": "electron-builder --win appx"
  },
  "build": {
    "appId": "Ihubsapp.stocksalesappdevelopemttype",
    "productName": "stocksalesappdevelopemttype",
    "directories": {
      "output": "dist",
      "buildResources": "build"
    },
    "files": [
      "**/*",
      "!src/**/*",
      "!output/**/*",
      "!certificate.pfx",
      "!tailwind.config.js"
    ],
    "win": {
      "target": [
        {
          "target": "appx",
          "arch": [
            "x64"
          ]
        }
      ],
      "icon": "stockapp.ico"
    },
    "appx": {
      "identityName": "Ihubsapp.stocksalesappdevelopemttype",
      "publisher": "CN=6DD6C43D-7610-4460-9766-F0629B188FAD",
      "publisherDisplayName": "I hubs tech",
      "displayName": "stocksalesappdevelopemttype",
      "backgroundColor": "#ffffff",
      "capabilities": [
        "internetClient",
        "privateNetworkClientServer"
      ]
    }
  },
  "dependencies": {
    "adm-zip": "^0.5.16",
    "archiver": "^7.0.1",
    "archiver-zip-encrypted": "^2.0.0",
    "bonjour": "^3.3.0",
    "chart.js": "^4.5.0",
    "chartjs-chart-financial": "^0.2.1",
    "d3": "^7.9.0",
    "docx": "^9.5.1",
    "express": "^4.18.2",
    "extract-zip": "^2.0.1",
    "geoip-lite": "^1.4.10",
    "html2canvas": "^1.4.1",
    "ipp": "^1.1.0",
    "jspdf": "^4.0.0",
    "lucide-react": "^0.568.0",
    "multer": "^2.0.2",
    "multicast-dns": "^7.2.5",
    "node-cron": "^4.2.1",
    "node-forge": "^1.3.3",
    "node-machine-id": "^1.1.12",
    "nodemailer": "^7.0.12",
    "pdfkit": "^0.17.1",
    "ws": "^8.18.2",
    "ws-discovery": "^0.0.2",
    "wss": "^0.0.2",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@tailwindcss/cli": "^4.1.18",
    "autoprefixer": "^10.4.23",
    "electron": "^36.3.2",
    "electron-builder": "^26.7.0",
